{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "import/order": [
      "warn",
      {
        "alphabetize": {
          "caseInsensitive": true,
          "order": "asc"
        },
        "groups": [
          "builtin",
          "external",
          "internal",
          [
            "sibling",
            "parent"
          ],
          "index",
          "type",
          "object",
          "unknown"
        ],
        "newlines-between": "always"
      }
    ]
  }
}